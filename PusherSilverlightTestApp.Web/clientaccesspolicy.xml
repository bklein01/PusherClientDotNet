<?xml version="1.0" encoding="utf-8" ?>
<access-policy>
  <cross-domain-access>
    <policy>
      <allow-from>
        <domain uri="http://*"/>
      </allow-from>
      <grant-to>
        <resource path="/auth" include-subpaths="false"/>
      </grant-to>
    </policy>
  </cross-domain-access>
</access-policy>
